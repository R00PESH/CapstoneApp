package training.iqgateway.repo;

import org.springframework.data.mongodb.repository.MongoRepository;

import training.iqgateway.model.DoctorEO;
import java.util.List;


public interface DoctorRepository extends MongoRepository<DoctorEO, String> {
	

	DoctorEO  findByDocId(String docId);
	
	DoctorEO findByName(String name);
	
	List<DoctorEO> findByHosId(String hosId);
	
    List<DoctorEO> findByAvailabilityStatus(String availabilityStatus);
    
    List<DoctorEO> findBySpeciality(String speciality);
    
    List<DoctorEO> findByYearsOfExpGreaterThanEqual(double yearsOfExperience);
    
    List<DoctorEO> findByRatingGreaterThanEqual(double rating);
    

}
